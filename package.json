{
  "name": "kyte-one-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create Kyte-Research/kyte-one-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create suryasingh/kyte-one-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
